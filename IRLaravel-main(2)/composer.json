{
    "name": "serayusoft/serayucms",
    "description": "CMS with Laravel Framework.",
    "keywords": ["CMS", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.2.0",
        "intervention/image": "2.4.*",
        "laravelcollective/html": "5.5.*",
        "infyomlabs/laravel-generator": "5.2.x-dev",
        "infyomlabs/adminlte-templates": "5.2.x-dev",
        "doctrine/dbal": "2.3.*",
        "barryvdh/laravel-debugbar": "^2.3",
        "infinety-es/filemanager": "2.2",
        "dimsav/laravel-translatable": "6.*",
        "mcamara/laravel-localization": "1.3.19",
        "tymon/jwt-auth": "^0.5.12",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "wisembly/elephant.io": "^3.3",
        "guzzlehttp/guzzle": "^6.3.3",
        "phpoffice/phpexcel": "^1.8",
        "league/fractal": "^0.17.0",
        "barryvdh/laravel-cors": "0.11.0",
        "davejamesmiller/laravel-breadcrumbs": "4.x",
        "mariuzzo/laravel-js-localization": "^1.7",
        "maatwebsite/excel": "^3.1",
        "mollie/mollie-api-php": "^2.0",
        "barryvdh/laravel-snappy": "^0.4.8",
        "webpatser/laravel-uuid": "^4.0",
        "alariva/laravel-email-domain-blacklist": "^1.0.1",
        "ezyang/htmlpurifier": "^4.13",
        "socialiteproviders/apple": "3.0.1",
        "lcobucci/jwt": "3.4",
        "trantuanson1991/php-fcm-v1": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "barryvdh/laravel-ide-helper": "^2.6"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/errors.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "minimum-stability": "dev",
        "prefer-stable": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
