{
    "name": "pan-socket-server",
    "version": "1.0.0",
    "description": "football project socket server",
    "main": "index.js",
    "scripts": {
        "babel": "babel-node",
        "start": "apidoc -i src/app/ -o doc/ && nodemon --exec yarn babel -- ./src/index.js",
        "server": "yarn babel ./index.js",
        "build": "apidoc -i src/app/ -o build/doc/ && babel src -d build --minified --compact=auto && yarn copy",
        "copy": "cp -rf fixtures/ build/ && cp -rf .env build/"
    },
    "keywords": [
        "pan689",
        "football"
    ],
    "license": "ISC",
    "dependencies": {
        "acl": "^0.4.11",
        "app-module-path": "^2.2.0",
        "async": "^2.1.5",
        "body-parser": "^1.18.2",
        "chalk": "^2.3.2",
        "cors": "^2.8.4",
        "express": "^4.16.3",
        "express-jwt": "^5.3.1",
        "express-session": "^1.15.6",
        "express-validator": "^5.0.3",
        "joi": "^13.3.0",
        "jsonwebtoken": "^8.2.1",
        "lodash": "^4.17.5",
        "mocha": "^5.0.5",
        "morgan": "^1.9.0",
        "multer": "^1.3.0",
        "pg": "^7.4.1",
        "socket.io": "~2",
        "swagger-express": "^1.0.5",
        "underscore": "^1.9.0",
        "uuid": "^3.2.1",
        "validator": "^9.4.1",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.49",
        "@babel/core": "^7.0.0-beta.49",
        "@babel/node": "^7.0.0-beta.49",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
        "@babel/preset-env": "^7.0.0-beta.49",
        "apidoc": "^0.18.0",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^8.0.0-beta",
        "babel-plugin-module-resolver": "^3.1.1",
        "bluebird": "^3.5.1",
        "chai": "^4.1.2",
        "chai-http": "^4.0.0",
        "dotenv": "^5.0.1",
        "nodemon": "^1.17.2"
    },
    "engines": {
        "node": ">=8.6.0"
    }
}
