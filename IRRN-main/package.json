{
    "name": "ItsReady",
    "version": "0.0.1",
    "private": true,
    "reactNativePermissionsIOS": [
        "Camera",
        "LocationWhenInUse",
        "MediaLibrary",
        "Notifications",
        "PhotoLibrary"
    ],
    "scripts": {
        "android:clean": "cp .env.dev .env && rm -rf android/app/build && cd android && ./gradlew clean && cd ..",
        "android_prod": "cp .env.prod .env && npx react-native run-android --variant 'prodDebug'",
        "android_dev": "cp .env.dev .env && npx react-native run-android --variant 'devDebug'",
        "android_stag": "cp .env.stag .env && npx react-native run-android --variant 'stagingDebug'",
        "android_template_dev": "cp .env.template.dev .env && npx react-native run-android --variant 'templateDevDebug'",
        "android_template_stag": "cp .env.template.stag .env && npx react-native run-android --variant 'templateStagingDebug'",
        "android_group_dev": "cp .env.group.dev .env && npx react-native run-android --variant 'groupDevDebug'",
        "android_group_stag": "cp .env.group.stag .env && npx react-native run-android --variant 'groupStagingDebug'",
        "android:dev:debug:apk": "cp .env.dev .env && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew clean && ./gradlew assembleDevDebug && cd ..",
        "android:dev:release:apk": "cp .env.dev .env && cd android && ./gradlew assembleDevRelease && cd ..",
        "android:stag:release:apk": "cp .env.stag .env && cd android && ./gradlew assembleStagingRelease && cd ..",
        "android:template_dev:release:apk": "cp .env.template.dev .env && cd android && ./gradlew assembleTemplateDevRelease && cd ..",
        "android:template_stag:release:apk": "cp .env.template.stag .env && rm -rf android/app/build && cd android && ./gradlew assembleTemplateStagingRelease && cd ..",
        "android:template_stag_brightest:release:apk": "cp envs/.env.template.brightest .env && rm -rf android/app/build && cd android && ./gradlew assembleBrightestRelease && cd ..",
        "android:template_stag_brightest_playground:release:apk": "cp envs/.env.template.brightestplayground .env && rm -rf android/app/build && cd android && ./gradlew assembleBrightestPlaygroundRelease && cd ..",
        "android:group_dev:release:apk": "cp .env.group.dev .env && cd android && ./gradlew assembleGroupDevRelease && cd ..",
        "android:group_stag:release:apk": "cp .env.group.stag .env && rm -rf android/app/build && cd android && ./gradlew assembleGroupStagingRelease && cd ..",
        "android:all_dev:release:apk": "rm -rf android/app/build && cd android && ./gradlew clean && cd .. && yarn android:dev:release:apk && yarn android:template_dev:release:apk && yarn android:group_dev:release:apk",
        "android:all_stag:release:apk": "yarn android:stag:release:apk && yarn android:template_stag:release:apk && yarn android:group_stag:release:apk",
        "android:prod:aab": "cp .env.prod .env && rm -rf android/app/build && cd android && ./gradlew bundleProdRelease && cd ..",
        "android:readyfreddy:aab": "cp envs/.env.template.readyfreddy .env && rm -rf android/app/build && cd android && ./gradlew bundleReadyfreddyRelease && cd ..",
        "ios_dev": "npx react-native run-ios --scheme 'ItsReady_Dev'",
        "ios_stag": "npx react-native run-ios --scheme 'ItsReady_Staging'",
        "ios_prod": "npx react-native run-ios --scheme 'ItsReady'",
        "ios_template_dev": "npx react-native run-ios --scheme 'ItsReady_Template_Dev'",
        "ios_template_stag": "npx react-native run-ios --scheme 'ItsReady_Template_Staging'",
        "ios_group_dev": "npx react-native run-ios --scheme 'ItsReady_Group_Dev'",
        "ios_group_stag": "npx react-native run-ios --scheme 'ItsReady_Group_Staging'",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --fix",
        "postinstall": "npx react-native setup-ios-permissions && patch-package",
        "prepare": "husky install"
    },
    "dependencies": {
        "@expo/react-native-action-sheet": "4.0.0",
        "@invertase/react-native-apple-authentication": "2.2.2",
        "@notifee/react-native": "5.7.0",
        "@pietile-native-kit/keyboard-aware-scrollview": "^1.5.0",
        "@react-native-async-storage/async-storage": "1.18.2",
        "@react-native-community/hooks": "2.8.1",
        "@react-native-community/push-notification-ios": "^1.10.1",
        "@react-native-firebase/analytics": "16.4.1",
        "@react-native-firebase/app": "16.4.1",
        "@react-native-firebase/messaging": "16.4.1",
        "@react-native-google-signin/google-signin": "8.2.1",
        "@react-navigation/bottom-tabs": "6.4.0",
        "@react-navigation/native": "6.0.13",
        "@react-navigation/native-stack": "6.9.1",
        "@react-navigation/stack": "^6.3.8",
        "@reduxjs/toolkit": "1.8.6",
        "@shopify/flash-list": "^1.7.3",
        "axios": "1.4.0",
        "dayjs": "1.11.6",
        "formik": "2.2.9",
        "i18next": "22.0.4",
        "lodash": "4.17.21",
        "moment": "2.29.4",
        "qs": " 6.11.0",
        "react": "18.2.0",
        "react-content-loader": "6.2.0",
        "react-i18next": "12.0.0",
        "react-native": "^0.72.0",
        "react-native-code-push": "^8.1.0",
        "react-native-config": "1.4.11",
        "react-native-device-info": "^10.6.0",
        "react-native-fast-image": "8.6.3",
        "react-native-fast-shadow": "^0.1.0",
        "react-native-fbsdk-next": "11.2.1",
        "react-native-geolocation-service": "5.3.1",
        "react-native-gesture-handler": "2.14.0",
        "react-native-image-crop-picker": "0.38.1",
        "react-native-inappbrowser-reborn": "^3.7.0",
        "react-native-keyboard-aware-scroll-view": "0.9.5",
        "react-native-linear-gradient": "2.6.2",
        "react-native-localize": "2.2.4",
        "react-native-maps": "1.7.1",
        "react-native-modal": "13.0.1",
        "react-native-pager-view": "6.0.2",
        "react-native-permissions": "^3.8.4",
        "react-native-popover-view": "5.1.7",
        "react-native-push-notification": "^8.1.1",
        "react-native-reanimated": "3.6.1",
        "react-native-safe-area-context": "^4.5.3",
        "react-native-screens": "^3.21.0",
        "react-native-svg": "^13.9.0",
        "react-native-swiper-flatlist": "^3.0.18",
        "react-native-tab-view": "^3.3.0",
        "react-native-toast-notifications": "^3.3.1",
        "react-native-webview": "^13.2.2",
        "react-redux": "^8.0.4",
        "react-use": "^17.4.0",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.2.1",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.1",
        "@react-native-community/eslint-config": "^3.1.0",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.11",
        "@tsconfig/react-native": "^3.0.0",
        "@types/jest": "^29.2.1",
        "@types/lodash": "^4.14.187",
        "@types/qs": "^6.9.7",
        "@types/react": "^18.0.24",
        "@types/react-native": "^0.70.6",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "babel-jest": "^29.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "deprecated-react-native-prop-types": "^2.3.0",
        "eslint": "^8.26.0",
        "eslint-plugin-ft-flow": "^2.0.1",
        "eslint-plugin-jest": "^27.1.3",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.1",
        "jest": "^29.2.2",
        "metro-react-native-babel-preset": "0.76.8",
        "patch-package": "^6.5.0",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "^2.7.1",
        "react-native-svg-transformer": "^1.0.0",
        "react-test-renderer": "18.2.0",
        "typescript": "4.8.4"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}